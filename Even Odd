import java.util.*;
class Main{
    // Check the number is Even or Odd
   static int evenOrodd(int number) {
        if(number%2 == 0) {
            return 1;
        }
        else {
            return 0;
        }
    }
    
   /* static boolean evenOrodd(int number) {
        return number % 2 == 0;
    }*/
    
    public static void main(String[] args) {
        int num = 896;
        // call the static method
        
        //if(evenOrodd(num)) {
        if(evenOrodd(num) == 1) {
            System.out.println("Even");
        }
        else{
            System.out.println("Odd");
        }
    }
}
